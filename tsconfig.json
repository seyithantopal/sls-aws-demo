{
	"compilerOptions": {
		"strict": true,
		"module": "CommonJS",
		"declaration": true,
		"removeComments": true,
		"allowSyntheticDefaultImports": true,
		"experimentalDecorators": true,
		"target": "ESNext",
		"sourceMap": true,
		"outDir": "./dist",
		"baseUrl": "./src",
		"incremental": false,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"paths": {
			"@users/*": ["./users/*"]
		}
	},
	"include": ["./src/**/*.ts"]
}
